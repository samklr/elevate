{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": false,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "target": "es2018",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom",
      "webworker"
    ],
    "baseUrl": "./",
    "paths": {
      "@elevate/shared/constants/*": [
        "../appcore/modules/shared/constants/*"
      ],
      "@elevate/shared/enums/*": [
        "../appcore/modules/shared/enums/*"
      ],
      "@elevate/shared/data/*": [
        "../appcore/modules/shared/data/*"
      ],
      "@elevate/shared/sync/*": [
        "../appcore/modules/shared/sync/*"
      ],
      "@elevate/shared/models/*": [
        "../appcore/modules/shared/models/*"
      ],
      "@elevate/shared/resolvers/*": [
        "../appcore/modules/shared/resolvers/*"
      ],
      "@elevate/shared/exceptions/*": [
        "../appcore/modules/shared/exceptions/*"
      ],
      "@elevate/shared/tools/*": [
        "../appcore/modules/shared/tools/*"
      ]
    },
    "skipLibCheck": true
  }
}
